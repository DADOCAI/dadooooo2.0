{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "header", "key": "accept", "value": "text/html.*" }
      ],
      "missing": [
        { "type": "file" }
      ],
      "destination": "/index.html"
    }
  ]
}